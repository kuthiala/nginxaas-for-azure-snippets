{
    "$schema": "https://schema.management.azure.com/schemas/2014-04-01-preview/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "westus",
            "type": "string"
        },
        "nginxDeploymentName": {
            "value": "myDeployment",
            "type": "string"
        },
        "sku": {
            "value": "standardv2_Monthly",
            "type": "string"
        },
        "publicIPName": {
            "value": "myPublicIP",
            "type": "string"
        },
        "subnetName": {
            "value": "mySubnet",
            "type": "string"
        },
        "virtualNetworkName": {
            "value": "myVirtualNetwork",
            "type": "string"
        },
        "capacity": {
            "value": 50,
            "type": "int"
        },
        "diagnosticSettingName": {
            "type": "string",
            "value": "myDiagnosticSetting"
        },
        "diagnosticSettingStorageAccountID": {
            "type": "string",
            "value": "/subscriptions/mysubscription/resourceGroups/myresourcegroup/providers/Microsoft.Storage/storageAccounts/mystorageaccount"
        }
    }
}
