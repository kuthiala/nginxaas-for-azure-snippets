    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "westus",
            "type": "string"
        },
        "nginxDeploymentName": {
            "value": "myDeployment",
            "type": "string"
        },
        "sku": {
            "value": "standardv2_Monthly",
            "type": "string"
