{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "westus",
            "type": "string"
        },
        "nginxDeploymentName": {
            "value": "myDeployment",
            "type": "string"
        },
        "sku": {
            "value": "standardv2_Monthly",
            "type": "string"
        },
        "publicIPName": {
            "value": "myPublicIP",
            "type": "string"
        },
        "subnetName": {
            "value": "mySubnet",
            "type": "string"
        },
        "userAssignedIdentityName": {
            "value": "myManagedIdentity",
            "type": "string"
        },
        "virtualNetworkName": {
            "value": "myVirtualNetwork",
            "type": "string"
        },
        "enableMetrics": {
            "value": false,
            "type": "bool"
        },
        "capacity": {
            "value": 50,
            "type": "int"
        }
    }
}
